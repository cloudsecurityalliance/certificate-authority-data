    <table class="layout-table change-head assignee" id="ch-75" role="presentation">
      <tr>
          <td rowspan="2" class="change-gravatar"><div class="vcard vcard_661011" id="a102062958_661011"><img src="https://secure.gravatar.com/avatar/01529deaafc1a9fb5aa0a376a7bea560?d=mm&size=64" class="gravatar" width="32" height="32">

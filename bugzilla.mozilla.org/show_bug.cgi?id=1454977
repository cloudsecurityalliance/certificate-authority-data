       data-tags="">
    
    <table class="layout-table change-head " id="ch-12" role="presentation">
      <tr>
          <td rowspan="2" class="change-gravatar"><div class="vcard vcard_310524" ><img src="https://secure.gravatar.com/avatar/faede0fc9d625b79b41f567407337db6?d=mm&size=64" class="gravatar" width="32" height="32">
